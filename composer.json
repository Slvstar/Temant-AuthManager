{
    "name": "temant/auth-manager",
    "autoload": {
        "psr-4": {
            "Temant\\AuthManager\\": "Src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Temant\\AuthManager\\Tests\\": "tests/"
        }
    },
    "require": {
        "temant/cookie-manager": "*",
        "temant/session-manager": "*",
        "temant/settings-manager": "*",
        "doctrine/orm": "*",
        "doctrine/dbal": "*",
        "symfony/cache": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "symfony/var-dumper": "^7.2",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "analyse": "./vendor/bin/phpstan",
        "test": "./vendor/bin/phpunit"
    }
}